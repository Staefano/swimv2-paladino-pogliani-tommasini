<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">
	<persistence-unit name="swimv2">
		<jta-data-source>java:/SwimDS</jta-data-source>
		<class>it.polimi.swimv2.entity.Ability</class>
		<class>it.polimi.swimv2.entity.Feedback</class>
		<class>it.polimi.swimv2.entity.HelpRequest</class>
		<class>it.polimi.swimv2.entity.Message</class>
		<class>it.polimi.swimv2.entity.Notification</class>
		<class>it.polimi.swimv2.entity.User</class>
		<class>it.polimi.swimv2.entity.Friendship</class>
		<class>it.polimi.swimv2.entity.PendingToken</class>
		<class>it.polimi.swimv2.entity.AbilityRequest</class>
		<class>it.polimi.swimv2.entity.Comment</class>
		<class>it.polimi.swimv2.entity.UserImage</class>
		<properties>
			<property name="hibernate.hbm2ddl.auto" value="create-drop" />
		</properties>
	</persistence-unit>
</persistence>